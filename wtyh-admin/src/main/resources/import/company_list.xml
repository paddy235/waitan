<?xml version="1.0" encoding="UTF-8" ?>

<excel key="company_list" name="企业名单-导入模板">
    <sheet name="Sheet1" startRow="3" entity="com.bbd.wtyh.domain.CompanyDO"
           importHandler="com.bbd.bgo.service.imp.handler.CompanyListHandler">
        <rowHeader>
            <column mergeFirstRow="1" mergeLastRow="1" mergeFirstCol="1" mergeLastCol="5"
                    value="填写说明：本表格不可填写Excel公式；企业名称必填，不能超过40个汉字；"/>
        </rowHeader>
        <columns>
            <column name="企业名称" code="name" dataType="string" required="true"/>
            <column name="统一信用代码" code="creditCode" dataType="string" bgColor="#FFFF00"/>
            <column name="组织机构代码" code="organizationCode" dataType="string" bgColor="#FFFF00"/>
            <column name="行业类别" code="businessType" dataType="string"/>
        </columns>

    </sheet>

</excel>