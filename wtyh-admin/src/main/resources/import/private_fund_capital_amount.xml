           importHandler="com.bbd.bgo.service.imp.handler.PriFundCapitalAmountHandler">
        <rowHeader>
            <column mergeFirstRow="1" mergeLastRow="2" mergeFirstCol="1" mergeLastCol="3" align="LEFT"
                    value="填写说明：本表格不可填写Excel公式；&#x000A;私募机构类型必填，不能超过40个汉字；"/>
