<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bbd.wtyh.mapper.IndustryShanghaiAreaMapper">

    <insert id="save" parameterType="com.bbd.wtyh.domain.IndustryShanghaiAreaDO">
        INSERT INTO `industry_shanghai_area` (`date`, `area_name`, `area_num`, `create_by`, `create_date`, `update_by`, `update_date`)
        VALUES (#{date}, #{areaName}, #{areaNum}, #{createBy}, #{createDate}, #{updateBy}, #{updateDate});
    </insert>

    <delete id="deleteByDate" parameterType="java.lang.String">
        DELETE FROM industry_shanghai_area WHERE `date`=#{date}
    </delete>

</mapper>